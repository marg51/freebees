{
    "name": "freebees",
    "version": "0.0.1",
    "description": "API testing made easy",
    "main": "dist/index.js",
    "scripts": {
        "test": "jest",
        "prepublishOnly": "tsc"
    },
    "files": ["dist"],
    "author": "Laurent Margirier <laurent.margirier+npm@gmail.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/marg51/freebees"
    },
    "devDependencies": {
        "@types/es6-shim": "^0.31.35",
        "@types/jest": "^21.1.9",
        "@types/lodash": "^4.14.91",
        "@types/node": "^8.5.2",
        "@types/request": "2.0.9",
        "babel-jest": "^22.0.4",
        "jest": "^22.0.4",
        "ts-jest": "^22.0.0",
        "typescript": "^2.6.2"
    },
    "jest": {
        "transform": {
            ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        },
        "testEnvironment": "node",
        "testMatch": ["**/__tests__/**/*.ts", "**/?(*.)(spec|test).ts"],
        "moduleFileExtensions": ["ts", "tsx", "js", "json"]
    },
    "dependencies": {
        "cli-table": "^0.3.1",
        "jsonschema": "^1.2.2",
        "lodash": "^4.17.4",
        "prettyjson": "^1.2.1",
        "request": "^2.83.0"
    }
}
